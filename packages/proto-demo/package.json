    "@material-ui/icons": "^4.11.2",
    "@mscharley/bs-material-ui-icons": "^2.0.0",
    "bs-css-emotion": "^3.0.0",
    "bs-platform": "^9.0.2",
    "express": "^4.17.1",
    "protobufjs": "^6.8.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rescript": "^9.1.4",
    "rescript-express": "^0.4.1"
  }
