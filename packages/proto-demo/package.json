    "@babel/register": "^7.6.0",
    "@reproto/bs-protobuf": "^0.1.5",
    "@rescript/react": "^0.10.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "bs-platform": "^9.0.2",
    "enzyme": "^3.11.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^27.0.4",
    "jest-environment-node": "^27.0.3",
    "react-hot-loader": "^4.13.0",
